@inject SnackbarService SnackbarService

<MduiButton Color="@Colors.Theme.Primary" OnClick="@Show">
    OpenCustomSnackbar
</MduiButton>

@code {
    void Show()
    {
        var parameters = new ComponentParameters();
        parameters.Add("Message", "这是一个自定义的 Snackbar。");
        SnackbarService.ShowSnackbar<SnackbarContent>(parameters);
    }
}